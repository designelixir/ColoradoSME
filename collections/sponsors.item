
{.section item}
<div class="card-item">
  <div class="card-item-img card-section">
      <img src="{assetUrl}" style="width: 100%;">
  </div>
  <div class="card-item-info card-section">
     
        <h1 href="{fullUrl}" target="_blank">{title}</h1> 
      <p style="font-size: 14px;">{customContent.packagequantity}</p>
      
      <p>{customContent.packageprice} - <a href="mailto:alex@metsim.com" style="color: gray; font-style: italic;">Contact Alex Holtzapple for details.</a></p>
      <p>{excerpt}</p>
      <br>
      <a class="button-style dark-green-bg" href="mailto:alex@metsim.com">Contact alex@metsim.com for details</a>
  </div>
</div>

<div>

</div>


 

{.end}

